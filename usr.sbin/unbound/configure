printf %s "checking for the $m Python module... " >&6; }
        if ac_modulecheck_result1=`$PYTHON -c "import $m" 2>&1`; then
                { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
