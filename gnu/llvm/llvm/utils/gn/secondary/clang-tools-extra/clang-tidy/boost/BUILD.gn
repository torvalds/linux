static_library("boost") {
  output_name = "clangTidyBoostModule"
  configs += [ "//llvm/utils/gn/build:clang_code" ]
  deps = [
    "//clang-tools-extra/clang-tidy",
    "//clang-tools-extra/clang-tidy/utils",
    "//clang/lib/AST",
    "//clang/lib/ASTMatchers",
    "//clang/lib/Basic",
    "//clang/lib/Lex",
    "//llvm/lib/Support",
  ]
  sources = [
    "BoostTidyModule.cpp",
    "UseRangesCheck.cpp",
    "UseToStringCheck.cpp",
  ]
}
