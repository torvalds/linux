{
  "directory": "${path}",
  "command": "g++ -c -o main.o main.c -Wall -DDEBUG -Dvariable=value",
  "file": "${path}/main.c"
}
,
{
  "directory": "${path}",
  "command": "cc -c -o clean-one.o clean-one.c -Wall -DDEBUG \"-Dvariable=value with space\" -Iinclude",
