# only ac_max_sed_lines should be used.
: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='libssp'
PACKAGE_TARNAME='libssp'
PACKAGE_VERSION='1.0'
PACKAGE_STRING='libssp 1.0'
