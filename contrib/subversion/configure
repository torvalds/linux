        CFLAGS="$CFLAGS -pg"
        CXXFLAGS="$CXXFLAGS -pg"
        LT_LDFLAGS="$LT_LDFLAGS -pg"
      else
        as_fn_error $? "We only support --enable-gprof with GCC right now." "$LINENO" 5
      fi
    fi

