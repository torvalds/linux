	#!/bin/sh
	echo "[$@]"
	echo "I am running"
	echo "I am running ($$)" >> huh.out
	_EOF
	cat << _EOF > huh.tcsh
	#!/bin/tcsh -f

	./huh \
	    `#comment blah blah blah` \
	    parameter a \
	    `#comment blah blah blah` \
	    parameter b \
	    `#comment blah blah blah` \
	    parameter c
	echo ok
	_EOF
