	default n
	help
	  rockchip mpp vpu encoder v2.

config ROCKCHIP_MPP_IEP2
	tristate "IEP v2 device driver"
	depends on ROCKCHIP_MPP_SERVICE
	default n
	help
	  rockchip iep v2.
