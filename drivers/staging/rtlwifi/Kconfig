config R8822BE
	tristate "Realtek RTL8822BE Wireless Network Adapter"
	depends on PCI && MAC80211 && m
	select FW_LOADER
	---help---
	This is the staging driver for Realtek RTL8822BE 802.11ac PCIe
	wireless network adapters.

config RTLHALMAC_ST
	tristate
	depends on R8822BE
	default m

config RTLPHYDM_ST
	tristate
	depends on R8822BE
	default m

config RTLWIFI_DEBUG_ST
	bool
	depends on R8822BE
	default y
