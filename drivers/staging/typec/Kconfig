menu "USB Power Delivery and Type-C drivers"

if TYPEC_TCPM

config TYPEC_TCPCI
	tristate "Type-C Port Controller Interface driver"
	depends on I2C
	select REGMAP_I2C
	help
	  Type-C Port Controller driver for TCPCI-compliant controller.

source "drivers/staging/typec/fusb302/Kconfig"

endif

endmenu
