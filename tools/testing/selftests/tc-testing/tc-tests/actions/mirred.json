        "verifyCmd": "$TC actions list action mirred",
        "matchPattern": "action order [0-9]*: mirred \\(Egress Mirror to device lo\\).*index 1 ref",
        "matchCount": "1",
        "teardown": [
            "$TC actions flush action mirred"
        ]
    },
