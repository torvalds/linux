        "category": [
            "actions",
            "mirred"
        ],
        "setup": [
            [
                "$TC actions flush action mirred",
                0,
                1,
                255
            ]
        ],
        "cmdUnderTest": "$TC actions add action mirred egress mirror index 1 dev lo",
