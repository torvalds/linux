        "id": "ddd9",
        "name": "Add prio qdisc on egress",
        "category": [
            "qdisc",
            "prio"
        ],
        "setup": [
            "$IP link add dev $DEV1 type dummy || /bin/true"
        ],
        "cmdUnderTest": "$TC qdisc add dev $DEV1 handle 1: root prio",
