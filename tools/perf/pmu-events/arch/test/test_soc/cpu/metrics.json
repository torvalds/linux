  },
  {
    "MetricExpr": "ipc + M1",
    "MetricName": "M2"
  },
  {
    "MetricExpr": "1/M3",
    "MetricName": "M3"
  },
  {
    "MetricExpr": "64 * l1d.replacement / 1000000000 / duration_time",
