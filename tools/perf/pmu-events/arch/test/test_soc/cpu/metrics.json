    "MetricExpr": "l1i\\-loads\\-misses / inst_retired.any",
    "MetricName": "icache_miss_cycles"
  },
  {
    "MetricExpr": "(dcache_miss_cpi + icache_miss_cycles)",
    "MetricName": "cache_miss_cycles",
    "MetricGroup": "group1"
