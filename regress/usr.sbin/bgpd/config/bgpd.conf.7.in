	192.0.211.0/24 prefixlen 24 - 32,
	2222:69:c000::/48 prefixlen 48 - 64,
	0.0.0.0/0 or-longer,
	192.0.160.0/19 prefixlen >= 22,
	192.0.160.0/19 prefixlen = 32
}

prefix-set "test6_prefixes" {
	192.0.211.0/24 prefixlen 28 - 31,
	192.0.211.0/24 prefixlen 25 - 31,
