test[/-/] test/ 0x0 0
test[/-/] test/ 0x2 1
test[/-/] test/ 0x1d 0
# 'test[\/-/]' 'test/'; no match if APR_FNM_PATHNAME
test[\/-/] test/ 0x0 0
test[\/-/] test/ 0x2 1
test[\/-/] test/ 0x1d 0
# 'test[/-\/]' 'test/'; no match if APR_FNM_PATHNAME
test[/-\/] test/ 0x0 0
test[/-\/] test/ 0x2 1
test[/-\/] test/ 0x1d 0
# '/test' 'test'; no match
/test test 0x0 1
/test test 0x1f 1
# 'test' '/test'; no match
test /test 0x0 1
test /test 0x1f 1
# 'test/' 'test'; no match
test/ test 0x0 1
test/ test 0x1f 1
# 'test' 'test/'; match if FNM_LEADING_DIR
test test/ 0x0 1
test test/ 0x17 1
test test/ 0x8 0
# '\/test' '/test'; match unless FNM_NOESCAPE
\/test /test 0x0 0
\/test /test 0x1 1
\/test /test 0x1e 0
# '*test' '/test'; match unless FNM_PATHNAME
*test /test 0x0 0
*test /test 0x2 1
*test /test 0x1d 0
# '/*/test' '/test'; no match
/*/test /test 0x0 1
/*/test /test 0x1f 1
# '/*/test' '/test/test'; match always
/*/test /test/test 0x0 0
/*/test /test/test 0x1f 0
# 'test/this' 'test/'; match never
test/this test/ 0x0 1
test/this test/ 0x1f 1
# 'test/' 'test/this'; match never
test/ test/this 0x0 1
test/ test/this 0x1f 1
# 'test*/this' 'test/this'; match always
test*/this test/this 0x0 0
test*/this test/this 0x1f 0
# 'test*/this' 'test/that'; match never
test*/this test/that 0x0 1
test*/this test/that 0x1f 1
# 'test/*this' 'test/this'; match always
test/*this test/this 0x0 0
test/*this test/this 0x1f 0
# '.*' '.this'; match always
.* .this 0x0 0
.* .this 0x1f 0
# '*' '.this'; fails if FNM_PERIOD
* .this 0x0 0
* .this 0x4 1
* .this 0x1b 0
# '?this' '.this'; fails if FNM_PERIOD
?this .this 0x0 0
?this .this 0x4 1
?this .this 0x1b 0
# '[.]this' '.this'; fails if FNM_PERIOD
[.]this .this 0x0 0
[.]this .this 0x4 1
[.]this .this 0x1b 0
