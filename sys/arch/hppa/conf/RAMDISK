ipgphy*		at mii?			# IC Plus IP1000A PHYs
ukphy*		at mii?			# generic unknown PHYs

wsdisplay*	at sti?
wskbd*		at pckbd? mux 1
wskbd*		at hilkbd? mux 1

pciide*		at pci? flags 0x0000
wdc*		at pcmcia?
