&i2c0 {
	status = "okay";

	eeprom@51 {
		compatible = "atmel,24c64";
		reg = <0x51>;
	};

	pca1: pca9552@62 {
		compatible = "nxp,pca9552";
		reg = <0x62>;
		#address-cells = <1>;
		#size-cells = <0>;

		gpio-controller;
		#gpio-cells = <2>;

		gpio-line-names =
			"presence-ps0",
			"presence-ps1",
			"presence-ps2",
			"presence-ps3",
			"presence-pdb",
			"presence-tpm",
			"", "",
			"presence-cp0",
			"presence-cp1",
			"presence-cp2",
			"presence-cp3",
			"presence-dasd",
			"presence-lcd-op",
			"presence-base-op",
			"";
	};
};

