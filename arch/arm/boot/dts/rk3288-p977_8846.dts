	supports-emmc;
        bootpart-no-access;

	supports-DDR_MODE;
	caps2-mmc-hs200;

        ignore-pm-notify;
		keep-power-in-suspend;
    	status = "okay";
};
    
&sdmmc {
	clock-frequency = <50000000>;
	clock-freq-min-max = <400000 50000000>;	            
	    
	supports-highspeed;
	supports-sd;
	broken-cd;
	card-detect-delay = <200>;

	ignore-pm-notify;
        keep-power-in-suspend;
	
        vmmc-supply = <&ldo1_reg>;
	status = "okay";
};
		
