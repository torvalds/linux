/dts-v1/;
/include/ "dbx5x0.dtsi"
/include/ "href.dtsi"
/include/ "stuib.dtsi"

/ {
	model = "ST-Ericsson HREF (v60+) platform with Device Tree";
	compatible = "st-ericsson,hrefv60+", "st-ericsson,u8500";

	gpio_keys {
		button@1 {
			gpios = <&gpio6 25 0x4>;
		};
	};

	soc-u9500 {
		i2c@80110000 {
			bu21013_tp@0x5c {
				reset-gpio = <&gpio4 15 0x4>;
			};
		};
	};
};
