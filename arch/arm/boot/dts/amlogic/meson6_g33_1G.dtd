				touch_gslx680:gslx680{
					touch_name = "gslx680";
					i2c_bus = "i2c_bus_a";
					reg = <0x40>;
					ic_type = <0>;
					irq = <1>;
					irq_edge = "GPIO_IRQ_RISING";
					gpio_interrupt = "GPIOA_16";
					gpio_reset = "GPIOC_3";
					xres = <1024>;
					yres = <600>;
					pol = <0x4>;
					max_num = <5>;
					fw_file = "/etc/touch/gslx680.fw";
