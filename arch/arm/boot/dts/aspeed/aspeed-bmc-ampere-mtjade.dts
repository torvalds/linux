	/*G0-G7*/	"host0-ready","host0-shd-req-n","host0-shd-ack-n",
			"s0-overtemp-n","","","","",
	/*H0-H7*/	"uart1-mode1","uart2-mode1","uart3-mode1","uart4-mode1",
