	/*K0-K7*/	"","","","","","","","",
	/*L0-L7*/	"","","","host0-sysreset-n","s0-spi-auth-fail-n","","","",
	/*M0-M7*/	"","","","","s0-i2c9-alert-n","s1-i2c9-alert-n","","",
