/dts-v1/;
#include "r7s72100.dtsi"

/ {
	model = "Genmai";
	compatible = "renesas,genmai-reference", "renesas,r7s72100";

	chosen {
		bootargs = "console=ttySC2,115200 ignore_loglevel rw root=/dev/nfs ip=dhcp";
	};

	memory {
		device_type = "memory";
		reg = <0x08000000 0x08000000>;
	};

	lbsc {
		#address-cells = <1>;
		#size-cells = <1>;
	};
};
