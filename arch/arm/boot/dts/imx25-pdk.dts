/dts-v1/;
/include/ "imx25.dtsi"

/ {
	model = "Freescale i.MX25 Product Development Kit";
	compatible = "fsl,imx25-pdk", "fsl,imx25";

	memory {
		reg = <0x80000000 0x4000000>;
	};
};

&uart1 {
	status = "okay";
};

&fec {
	phy-mode = "rmii";
	status = "okay";
};

&nfc {
	nand-on-flash-bbt;
	status = "okay";
};
