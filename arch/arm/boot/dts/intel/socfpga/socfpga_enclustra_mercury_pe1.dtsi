&i2c_encl {
	status = "okay";

	eeprom@57 {
		status = "okay";
		compatible = "microchip,24c128";
		reg = <0x57>;
		pagesize = <64>;
		label = "user eeprom";
		address-width = <16>;
	};

	lm96080: temperature-sensor@2f {
		status = "okay";
		compatible = "national,lm80";
		reg = <0x2f>;
	};

	si5338: clock-controller@70 {
		compatible = "silabs,si5338";
		reg = <0x70>;
	};

};

&i2c_encl_fpga {
	status = "okay";
};
