&i2c_encl {
	i2c-mux@74 {
		status = "okay";
		compatible = "nxp,pca9547";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x74>;

		i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			eeprom@56 {
				status = "okay";
				compatible = "microchip,24c128";
				reg = <0x56>;
				pagesize = <64>;
				label = "user eeprom";
				address-width = <16>;
			};

			lm96080: temperature-sensor@2f {
				status = "okay";
				compatible = "national,lm80";
				reg = <0x2f>;
			};

			pcal6416: gpio@20 {
				status = "okay";
				compatible = "nxp,pcal6416";
				reg = <0x20>;
				gpio-controller;
				#gpio-cells = <2>;
			};
		};
	};
};

&i2c_encl_fpga {
	status = "okay";

	i2c-mux@75 {
		status = "okay";
		compatible = "nxp,pca9547";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x75>;
	};
};
