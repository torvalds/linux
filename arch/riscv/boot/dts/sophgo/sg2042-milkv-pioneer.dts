#include "sg2042.dtsi"

/ {
	model = "Milk-V Pioneer";
	compatible = "milkv,pioneer", "sophgo,sg2042";

	chosen {
		stdout-path = "serial0";
	};
};

&cgi_main {
	clock-frequency = <25000000>;
};

&cgi_dpll0 {
	clock-frequency = <25000000>;
};

&cgi_dpll1 {
	clock-frequency = <25000000>;
};

&emmc {
	bus-width = <4>;
	no-sdio;
	no-sd;
	non-removable;
	wp-inverted;
	status = "okay";
};

&sd {
	bus-width = <4>;
	no-sdio;
	no-mmc;
	wp-inverted;
	status = "okay";
};

&uart0 {
	status = "okay";
};
