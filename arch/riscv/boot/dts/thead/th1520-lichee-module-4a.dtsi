/dts-v1/;

#include "th1520.dtsi"

/ {
	model = "Sipeed Lichee Module 4A";
	compatible = "sipeed,lichee-module-4a", "thead,th1520";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x00000000 0x2 0x00000000>;
	};
};

&osc {
	clock-frequency = <24000000>;
};

&osc_32k {
	clock-frequency = <32768>;
};

&aogpio {
	gpio-line-names = "", "", "",
			  "GPIO00",
			  "GPIO04";
};

&dmac0 {
	status = "okay";
};

&emmc {
	bus-width = <8>;
	max-frequency = <198000000>;
	mmc-hs400-1_8v;
	non-removable;
	no-sdio;
	no-sd;
	status = "okay";
};

&gpio0 {
	gpio-line-names = "", "", "", "", "", "", "", "", "", "",
			  "", "", "", "", "", "", "", "", "", "",
			  "", "", "", "",
			  "GPIO07",
			  "GPIO08",
			  "",
			  "GPIO01",
			  "GPIO02";
};

&gpio1 {
	gpio-line-names = "", "", "",
			  "GPIO11",
			  "GPIO12",
			  "GPIO13",
			  "GPIO14",
			  "", "", "", "", "", "", "", "", "", "",
			  "", "", "", "", "",
			  "GPIO06";
};

&gpio2 {
	gpio-line-names = "GPIO03",
			  "GPIO05";
};

&gpio3 {
	gpio-line-names = "", "",
			  "GPIO09",
			  "GPIO10";
};

&sdio0 {
	bus-width = <4>;
	max-frequency = <198000000>;
	status = "okay";
};
