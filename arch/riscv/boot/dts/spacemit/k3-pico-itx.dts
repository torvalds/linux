#include "k3.dtsi"

/ {
	model = "SpacemiT K3 Pico-ITX";
	compatible = "spacemit,k3-pico-itx", "spacemit,k3";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0";
	};

	memory@100000000 {
		device_type = "memory";
		reg = <0x1 0x00000000 0x4 0x00000000>;
	};
};

&uart0 {
	status = "okay";
};
