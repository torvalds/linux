/dts-v1/;
#include "jh7110-evb.dtsi"

/ {
	model = "StarFive JH7110 EVB";
	compatible = "starfive,jh7110-evb", "starfive,jh7110";
};

&vin_sysctl {
	pinctrl-names = "default";
	pinctrl-0 = <&dvp_pins>;
};

&hdmi_output {
	status = "okay";
};

&tda988x_pin {
	status = "okay";
};

&encoder {
	status = "diabled";
};

&mipi_dsi {
	status = "diabled";
};
