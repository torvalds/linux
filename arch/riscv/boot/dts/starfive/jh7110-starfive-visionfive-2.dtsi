/dts-v1/;
#include "jh7110-common.dtsi"

/ {
	aliases {
		ethernet1 = &gmac1;
	};
};

&gmac1 {
	phy-handle = <&phy1>;
	phy-mode = "rgmii-id";
	status = "okay";

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "snps,dwmac-mdio";

		phy1: ethernet-phy@1 {
			reg = <0>;
		};
	};
};

&mmc0 {
	non-removable;
};
