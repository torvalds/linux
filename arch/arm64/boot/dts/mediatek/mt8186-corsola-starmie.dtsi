	en_pp6000_mipi_disp_150ma_fixed_pins: en_pp6000-mipi-disp-150ma-fixed-pins {
		pins-en {
			pinmux = <PINMUX_GPIO154__FUNC_GPIO154>;
			output-low;
		};
	};

	panel_default_pins: panel-default-pins {
		pins-en {
			pinmux = <PINMUX_GPIO98__FUNC_GPIO98>;
			output-low;
