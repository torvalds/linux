				regulator-always-on;
			};

			mt6331_vrtc_reg: ldo-vrtc {
				regulator-name = "vrtc";
				regulator-min-microvolt = <2800000>;
				regulator-max-microvolt = <2800000>;
				regulator-ramp-delay = <0>;
				regulator-always-on;
			};

			mt6331_vdig18_reg: ldo-vdig18 {
				regulator-name = "vdig18";
