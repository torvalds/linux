		};

		mt6331rtc: rtc {
			compatible = "mediatek,mt6331-rtc";
		};

		mt6331keys: keys {
			compatible = "mediatek,mt6331-keys";
			power {
				linux,keycodes = <KEY_POWER>;
				wakeup-source;
			};
			home {
				linux,keycodes = <KEY_HOME>;
			};
		};
	};
