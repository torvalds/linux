	};

	pinctrl_tpm3: tpm3grp {
		fsl,pins = <
			IMX952_PAD_GPIO_IO12__WAKEUPMIX_TOP_TPM3_CH2		0x51e
		>;
	};

	pinctrl_tpm6: tpm6grp {
