		vdd_3v3_dis: regulator@6 {
			compatible = "regulator-fixed";
			reg = <6>;
			regulator-name = "VDD_DIS_3V3_LCD";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
			gpio = <&exp1 3 GPIO_ACTIVE_HIGH>;
			enable-active-high;
			vin-supply = <&vdd_3v3_sys>;
