	};

	sdh2_fast_pins_1: sdh2-fast-pins-1 {
		pinctrl-single,pins = <0x64 1>;
		pinctrl-single,drive-strength = <0x1800 0x1800>;
		pinctrl-single,bias-pullup = <0 0xc000 0 0xc000>;
