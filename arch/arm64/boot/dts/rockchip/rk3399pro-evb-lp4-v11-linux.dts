		lcd_rst_gpio: lcd-rst-gpio {
			rockchip,pins =
				<3 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	npu_clk {
		npu_ref_clk: npu-ref-clk {
		     rockchip,pins =
			     <0 RK_PA2 1 &pcfg_pull_none>;
	     };
	};

