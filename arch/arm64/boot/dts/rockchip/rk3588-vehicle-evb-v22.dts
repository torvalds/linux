		regulator-max-microvolt = <12000000>;
		enable-active-high;
		gpio = <&i2c5_nca9539_gpio 5 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vcc12v_dcin>;
		regulator-state-mem {
			regulator-off-in-suspend;
			regulator-suspend-microvolt = <12000000>;
