			reg = <0 0xfd58c318 0 0x4>;
			clocks = <&cru I2S0_8CH_MCLKOUT>;
			#clock-cells = <0>;
			clock-output-names = "i2s0_mclkout_to_io";
			rockchip,bit-shift = <0>;
			rockchip,bit-set-to-disable;
