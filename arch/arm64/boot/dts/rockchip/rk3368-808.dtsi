	pinctrl-names = "default";
	pinctrl-0 = <&i2s_2ch_bus>;
};

&io_domains {
	status = "okay";

	audio-supply = <&vcca_18>;
	dvp-supply = <&vcc_18>;
	flash0-supply = <&vcc18_flash>;
	gpio30-supply = <&vcc_io>;
	gpio1830-supply = <&vcc_io>;
	sdcard-supply = <&vccio_sd>;
	wifi-supply = <&vcc_io>;
