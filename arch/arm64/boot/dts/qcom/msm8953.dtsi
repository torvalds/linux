			clocks = <&apcscc APCS_CPU4_CLK>;
			clock-names = "cpu";
			next-level-cache = <&l2_1>;
			operating-points-v2 = <&cpu_opp>;
			power-domains = <&cpu_pd4>, <&cpr 1>;
			power-domain-names = "psci", "perf";
			#cooling-cells = <2>;
		};

		cpu5: cpu@101 {
			device_type = "cpu";
			compatible = "arm,cortex-a53";
			reg = <0x101>;
			enable-method = "psci";
			capacity-dmips-mhz = <1024>;
