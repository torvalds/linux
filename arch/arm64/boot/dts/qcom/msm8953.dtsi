			operating-points-v2 = <&cpu_opp>;
			power-domains = <&cpu_pd6>, <&cpr 1>;
			power-domain-names = "psci", "perf";
			#cooling-cells = <2>;
		};

		cpu7: cpu@103 {
			device_type = "cpu";
			compatible = "arm,cortex-a53";
			reg = <0x103>;
			enable-method = "psci";
			capacity-dmips-mhz = <1024>;
			interconnects = <&bimc MAS_APPS_PROC RPM_ACTIVE_TAG &bimc SLV_EBI RPM_ACTIVE_TAG>,
					<&apcscc APCS_MAS_CCI &apcscc APCS_SLV_BIMC>;
			clocks = <&apcscc APCS_CPU4_CLK>;
			clock-names = "cpu";
			next-level-cache = <&l2_1>;
			operating-points-v2 = <&cpu_opp>;
			power-domains = <&cpu_pd7>, <&cpr 1>;
			power-domain-names = "psci", "perf";
			#cooling-cells = <2>;
		};

		cpu-map {
			cluster0 {
				core0 {
					cpu = <&cpu0>;
				};
				core1 {
					cpu = <&cpu1>;
				};
				core2 {
					cpu = <&cpu2>;
				};
				core3 {
					cpu = <&cpu3>;
				};
			};

			cluster1 {
				core0 {
					cpu = <&cpu4>;
				};
				core1 {
					cpu = <&cpu5>;
				};
				core2 {
					cpu = <&cpu6>;
				};
				core3 {
					cpu = <&cpu7>;
				};
			};
		};

		idle-states {
			entry-method = "psci";

			cpu_pc: cpu-power-collapse {
				compatible = "arm,idle-state";
				idle-state-name = "cpu-pc";
				arm,psci-suspend-param = <0x40000003>;
				entry-latency-us = <90>;
				exit-latency-us = <180>;
				min-residency-us = <400>;
				local-timer-stop;
			};
		};

		cluster-idle-states {
			cluster_ret: cluster-retention {
				compatible = "domain-idle-state";
				idle-state-name = "cluster-ret";
				arm,psci-suspend-param = <0x41000033>;
				entry-latency-us = <120>;
				exit-latency-us = <200>;
				min-residency-us = <1200>;
				local-timer-stop;
			};

			cluster_gdhs: cluster-gdhs {
				compatible = "domain-idle-state";
				idle-state-name = "cluster-gdhs";
				arm,psci-suspend-param = <0x41000043>;
				entry-latency-us = <150>;
				exit-latency-us = <220>;
				min-residency-us = <1800>;
				local-timer-stop;
			};

			cluster_pc: cluster-power-collapse {
				compatible = "domain-idle-state";
				idle-state-name = "cluster-pc";
				arm,psci-suspend-param = <0x41000053>;
				entry-latency-us = <270>;
				exit-latency-us = <430>;
				min-residency-us = <2500>;
				local-timer-stop;
			};
		};

		l2_0: l2-cache-0 {
			compatible = "cache";
			cache-level = <2>;
			cache-unified;
		};

		l2_1: l2-cache-1 {
			compatible = "cache";
			cache-level = <2>;
			cache-unified;
		};
	};

