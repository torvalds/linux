			operating-points-v2 = <&cpu_opp>;
			power-domains = <&cpu_pd5>, <&cpr 1>;
			power-domain-names = "psci", "perf";
			#cooling-cells = <2>;
		};

		cpu6: cpu@102 {
			device_type = "cpu";
