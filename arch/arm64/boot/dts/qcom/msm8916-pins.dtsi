	cdc-dmic-lines {
		cdc_dmic_lines_act: dmic_lines_on {
			pinmux_dmic0_clk {
				function = "dmic0_clk";
				pins = "gpio0";
			};
			pinmux_dmic0_data {
				function = "dmic0_data";
				pins = "gpio1";
			};
			pinconf {
				pins = "gpio0", "gpio1";
				drive-strength = <8>;
			};
		};
		cdc_dmic_lines_sus: dmic_lines_off {
			pinmux_dmic0_clk {
				function = "dmic0_clk";
				pins = "gpio0";
			};
			pinmux_dmic0_data {
				function = "dmic0_data";
				pins = "gpio1";
			};
			pinconf {
				pins = "gpio0", "gpio1";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	wcnss_pin_a: wcnss-active {
		pinmux {
			pins = "gpio40", "gpio41", "gpio42", "gpio43", "gpio44";
			function = "wcss_wlan";
		};

		pinconf {
			pins = "gpio40", "gpio41", "gpio42", "gpio43", "gpio44";
			drive-strength = <6>;
			bias-pull-up;
		};
	};
};
