			output-high;
		};

		int-n-pins {
			pins = "gpio3";
			function = "gpio";
			bias-pull-up;
		};

		reset-n-pins {
			pins = "gpio52";
			function = "gpio";
			bias-disable;
		};
	};

	ts0_default: ts0-default-state {
		disable-pins {
			pins = "gpio75";
			function = "gpio";
			output-high;
		};

		int-n-pins {
			pins = "gpio51";
			function = "gpio";
			bias-pull-up;
		};

		reset-n-pins {
			/* Technically should be High-Z input */
