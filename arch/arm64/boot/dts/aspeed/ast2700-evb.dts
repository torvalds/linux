&pinctrl1 {
	pinctrl_i3c12_driving: i3c12_driving {
		pins = "AC24", "AD24";
		drive-strength = <3>;
	};

	pinctrl_i3c13_driving: i3c13_driving {
		pins = "AE23", "AE19";
		drive-strength = <3>;
	};

	pinctrl_i3c14_driving: i3c14_driving {
		pins = "AF23", "Y25";
		drive-strength = <3>;
	};

	pinctrl_i3c15_driving: i3c15_driving {
		pins = "AA25", "AF19";
		drive-strength = <3>;
	};
};

&gpio0 {
	pinctrl-0 = <&pinctrl_i3c12_driving &pinctrl_i3c13_driving
		     &pinctrl_i3c14_driving &pinctrl_i3c15_driving>;
	pinctrl-names = "default";
};

