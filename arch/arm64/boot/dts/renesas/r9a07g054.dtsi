			interrupt-names = "error", "rx", "tx";
			clocks = <&cpg CPG_MOD R9A07G054_RSPI0_CLKB>;
			resets = <&cpg R9A07G054_RSPI0_RST>;
			dmas = <&dmac 0x2e95>, <&dmac 0x2e96>;
			dma-names = "tx", "rx";
			power-domains = <&cpg>;
			num-cs = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
