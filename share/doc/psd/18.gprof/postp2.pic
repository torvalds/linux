circle diam .3i "\(ci"
circle diam .3i "\(ci" at 1st circle + (2i,0i)
circle diam .3i "\(bu" at 1st circle + (0.5i,-0.5i)
circle diam .3i "\(bu" at 2nd circle - (0.5i, 0.5i)
circle diam .3i "\(ci" at 1st circle - (0i,1i)
circle diam .3i "\(ci" at 5th circle + (1i,0i)
