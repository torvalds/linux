	{Buffer Full Ratio} i1
	{Buffer Empty Ratio} i1
	{Bus Inactivity Limit} i2
	{Disconnect Time Limit} i2
	{Connect Time Limit} i2
	{Maximum Burst Size} i2
	{Reserved} *t6
	{DTDC} t2
	{Reserved} *i1
	{Reserved} *i1
	{Reserved} *i1
}

0x15 "Extended";

0x16 "Extended Device-Type Specific";

