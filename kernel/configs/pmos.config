CONFIG_LOCALVERSION="-postmarketos-qcom-msm8953"
# CONFIG_LOCALVERSION_AUTO is not set

CONFIG_BLK_DEV_DM=y
CONFIG_BLK_DEV_INITRD=y
CONFIG_CGROUPS=y
CONFIG_CRYPTO_XTS=y
CONFIG_DEVTMPFS=y
CONFIG_DM_CRYPT=m
CONFIG_EXT4_FS=y
